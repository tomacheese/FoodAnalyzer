# =========================
# StyleCop.Analyzers スペーシングルール
# =========================
# SA1000: キーワードのスペーシングを強制しない
dotnet_diagnostic.SA1000.severity = none
# SA1001: コンマの後にはスペースを入れ、前にはスペースを入れない
dotnet_diagnostic.SA1001.severity = warning
# SA1002: セミコロンの後にはスペースを入れ、前にはスペースを入れない
dotnet_diagnostic.SA1002.severity = warning
# SA1003: シンボルの周りには適切にスペースを入れる
dotnet_diagnostic.SA1003.severity = warning
# SA1004: ドキュメント行は単一のスペースから始める
dotnet_diagnostic.SA1004.severity = warning
# SA1005: 単一行コメントは単一のスペースから始める
dotnet_diagnostic.SA1005.severity = warning
# SA1006: プリプロセッサキーワードの前にスペースを入れない
dotnet_diagnostic.SA1006.severity = warning
# SA1007: operator キーワードの後にはスペースを入れる
dotnet_diagnostic.SA1007.severity = warning
# SA1008: 開始括弧の前にはスペースを入れ、後にはスペースを入れない
dotnet_diagnostic.SA1008.severity = warning
# SA1009: 終了括弧のスペーシングを強制しない
dotnet_diagnostic.SA1009.severity = none
# SA1010: 開始角括弧のスペーシングを強制しない
dotnet_diagnostic.SA1010.severity = none
# SA1011: 終了角括弧のスペーシングを強制しない
dotnet_diagnostic.SA1011.severity = none
# SA1012: 開始中括弧の前後にはスペースを入れる
dotnet_diagnostic.SA1012.severity = warning
# SA1013: 終了中括弧の前後には適切にスペースを入れる
dotnet_diagnostic.SA1013.severity = warning
# SA1014: 開始ジェネリック角括弧の前にはスペースを入れず、後にもスペースを入れない
dotnet_diagnostic.SA1014.severity = warning
# SA1015: 終了ジェネリック角括弧の前後のスペースは正しく入れる
dotnet_diagnostic.SA1015.severity = warning
# SA1016: 開始属性括弧の前後のスペースは正しく入れる
dotnet_diagnostic.SA1016.severity = warning
# SA1017: 終了属性括弧の前後のスペースは正しく入れる
dotnet_diagnostic.SA1017.severity = warning
# SA1018: Null許容型シンボルの前にはスペースを入れない
dotnet_diagnostic.SA1018.severity = warning
# SA1019: メンバーアクセスシンボルの前後にはスペースを入れない
dotnet_diagnostic.SA1019.severity = warning
# SA1020: インクリメント/デクリメントシンボルの前後のスペースは正しく入れる
dotnet_diagnostic.SA1020.severity = warning
# SA1021: 負の符号の前後のスペースは正しく入れる
dotnet_diagnostic.SA1021.severity = warning
# SA1022: 正の符号の前後のスペースは正しく入れる
dotnet_diagnostic.SA1022.severity = warning
# SA1023: デリファレンスとアクセス演算子の前後のスペースは正しく入れる
dotnet_diagnostic.SA1023.severity = warning
# SA1024: コロンの前後のスペースは正しく入れる
dotnet_diagnostic.SA1024.severity = warning
# SA1025: コードには連続する複数のホワイトスペースを含めない
dotnet_diagnostic.SA1025.severity = warning
# SA1026: 暗黙的に型指定された配列割り当てでの new キーワードの後にスペースを含めない
dotnet_diagnostic.SA1026.severity = warning
# SA1027: タブを正しく使用する
dotnet_diagnostic.SA1027.severity = warning
# SA1028: コードは末尾のホワイトスペースを含めない
dotnet_diagnostic.SA1028.severity = warning

# =========================
# StyleCop.Analyzers 順序ルール
# =========================
# SA1200: usingディレクティブの配置場所を強制しない
dotnet_diagnostic.SA1200.severity = none
# SA1201: コード要素の順序を強制しない
dotnet_diagnostic.SA1201.severity = none
# SA1202: コード要素はアクセスレベル順に記述
dotnet_diagnostic.SA1202.severity = none
# SA1203: 定数とフィールドの順序を強制しない
dotnet_diagnostic.SA1203.severity = none
# SA1204: 静的要素とインスタンス要素の順序を強制しない
dotnet_diagnostic.SA1204.severity = none
# SA1205: partial要素はアクセス修飾子を宣言
dotnet_diagnostic.SA1205.severity = warning
# SA1206: 宣言キーワードの順序を強制しない（カスタム設定で対応）
dotnet_diagnostic.SA1206.severity = none
# SA1207: protected は internal の前に記述
dotnet_diagnostic.SA1207.severity = warning
# SA1208: System名前空間のusingディレクティブは他のusingディレクティブの前に配置
dotnet_diagnostic.SA1208.severity = warning
# SA1209: usingエイリアスディレクティブは他のusingディレクティブの後に配置
dotnet_diagnostic.SA1209.severity = warning
# SA1210: usingディレクティブは名前空間によってアルファベット順に並べる
dotnet_diagnostic.SA1210.severity = warning
# SA1211: usingエイリアスディレクティブはエイリアス名によってアルファベット順に並べる
dotnet_diagnostic.SA1211.severity = warning
# SA1212: プロパティアクセサは正しい順序で記述（getの後にset）
dotnet_diagnostic.SA1212.severity = warning
# SA1213: イベントアクセサは正しい順序で記述（addの後にremove）
dotnet_diagnostic.SA1213.severity = warning
# SA1214: 読み取り専用要素は非読み取り専用要素の前に記述
dotnet_diagnostic.SA1214.severity = none
# SA1215: インスタンスの読み取り専用要素はインスタンスの非読み取り専用要素の前に記述
dotnet_diagnostic.SA1215.severity = none
# SA1216: using static ディレクティブは正しい位置に配置
dotnet_diagnostic.SA1216.severity = warning
# SA1217: using static ディレクティブはアルファベット順に並べる
dotnet_diagnostic.SA1217.severity = warning

# =========================
